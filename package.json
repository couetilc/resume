{
  "name": "resume",
  "version": "1.0.0",
  "description": "",
  "html": "dist/index.html",
  "pdf": "dist/resume.pdf",
  "scripts": {
    "dev": "./bin/cli dev",
    "build": "./bin/cli build",
    "test": "./bin/cli test",
    "build:parcel": "parcel build src/index.html",
    "build:pdf": "'/Applications/Google Chrome.app/Contents/MacOS/Google Chrome' --headless --disable-gpu --print-to-pdf=dist/resume.pdf http://localhost:1234",
    "dev:parcel": "parcel src/index.html",
    "dev:pdf": "nodemon -w dist/index.html -x \"time npm run build:pdf\"",
    "server": "http-server -a localhost -p 1234 dist"
  },
  "repository": {
    "type": "git",
