  },
  "homepage": "https://github.com/couetilc/resume#readme",
  "dependencies": {
    "http-server": "^0.12.3",
    "nodemon": "^2.0.7",
    "parcel": "^2.0.0-beta.1",
    "sass": "^1.26.5"
  }
}
